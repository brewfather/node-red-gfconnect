    {
        "id": "a8c00bf5.937f48",
        "type": "function",
        "z": "9dea0b26.bbbe98",
        "name": "Decode Timer",
        "func": "function zeropad(num) {\n    var s = \"0\" + num;\n    return s.substr(s.length - 2);\n}\n\nvar timerActive = Boolean(msg.parameters[0] === \"1\");\nvar timerLeftMins = 0;\nvar timerStartMins = 0;\nvar timerLeftSecs = 0;\nvar timerFormatted = \"--\";\n\nif(timerActive) {\n    timerLeftSecs = Number(msg.parameters[3]);\n    timerLeftMins = Number(msg.parameters[1]) - 1 + Math.floor(timerLeftSecs / 60);\n    timerStartMins = Number(msg.parameters[2]);\n    timerLeftSecs = timerLeftSecs % 60;\n    if(timerLeftMins <= 59) {\n        timerFormatted = timerLeftMins.toString() + \":\" + zeropad(timerLeftSecs);\n    }\n    else {\n        var hours = Math.floor(timerLeftMins / 60);\n        var mins = timerLeftMins % 60;\n        timerFormatted = hours.toString() + \":\" + zeropad(mins) + \":\" + zeropad(timerLeftSecs);\n        //timerFormatted = \"_:__:__\";\n    }\n}\n\nnode.send([{'payload':timerActive}, null, null, null, null]);\nnode.send([null, {'payload':timerStartMins}], null, null, null);\nnode.send([null, null, {'payload':timerLeftMins}], null, null);\nnode.send([null, null, null, {'payload':timerLeftSecs}], null);\nnode.send([null, null, null, null, {'payload':timerFormatted}]);\n\n//return msg;",
        "outputs": 5,
        "noerr": 0,
        "x": 1320,
        "y": 700,
        "wires": [
            [],
            [],
            [],
            [],
            [
                "ec5bbfdd.5f86e"
            ]
        ],
        "outputLabels": [
            "active",
            "total",
            "mins",
            "secs",
            "formatted"
        ]
    },
